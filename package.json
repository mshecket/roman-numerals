{
  "name": "roman-numerals",
  "version": "0.1.0",
  "description": "Roman to Arabic and Arabic to Roman numeral converter",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mshecket/roman-numerals.git"
  },
  "keywords": [
    "roman",
    "numerals",
    "typescript",
    "jest",
    "node"
  ],
  "author": "Mike Shecket",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mshecket/roman-numerals/issues"
  },
  "homepage": "https://github.com/mshecket/roman-numerals#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.14",
    "jest": "^26.4.2",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {}
}
